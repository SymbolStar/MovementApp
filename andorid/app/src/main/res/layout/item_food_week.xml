<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@color/bg_white"
    android:orientation="vertical">

    <TextView
        android:id="@+id/textView21"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="六"
        android:textColor="@color/food_text_color"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/textView60"
        app:layout_constraintTop_toTopOf="@+id/textView60" />

    <TextView
        android:id="@+id/textView60"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="五"
        android:textColor="@color/food_text_color"
        app:layout_constraintRight_toLeftOf="@+id/textView21"
        app:layout_constraintLeft_toRightOf="@+id/textView59"
        app:layout_constraintTop_toTopOf="@+id/textView59" />

    <TextView
        android:id="@+id/textView59"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="四"
        android:textColor="@color/food_text_color"
        app:layout_constraintRight_toLeftOf="@+id/textView60"
        app:layout_constraintLeft_toRightOf="@+id/textView58"
        app:layout_constraintTop_toTopOf="@+id/textView58" />

    <TextView
        android:id="@+id/textView58"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="三"
        app:layout_constraintRight_toLeftOf="@+id/textView59"
        app:layout_constraintLeft_toRightOf="@+id/textView56"
        app:layout_constraintTop_toTopOf="@+id/textView56" />

    <TextView
        android:id="@+id/textView56"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="二"
        android:textColor="@color/food_text_color"
        app:layout_constraintRight_toLeftOf="@+id/textView58"
        app:layout_constraintLeft_toRightOf="@+id/textView55"
        app:layout_constraintTop_toTopOf="@+id/textView55" />

    <TextView
        android:id="@+id/textView55"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="一"
        android:textColor="@color/food_text_color"
        app:layout_constraintRight_toLeftOf="@+id/textView56"
        app:layout_constraintLeft_toRightOf="@+id/textView22"
        app:layout_constraintTop_toTopOf="@+id/textView22" />

    <TextView
        android:id="@+id/textView22"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="日"
        android:textColor="@color/food_text_color"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@+id/textView55"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="8dp" />

    <TextView
        android:id="@+id/tv_tue"
        android:layout_width="25dp"
        android:layout_height="25dp"
        android:layout_marginTop="8dp"
        android:text="18"
        android:gravity="center"
        android:textColor="@color/text_color"
        app:layout_constraintTop_toBottomOf="@+id/textView56"
        app:layout_constraintRight_toRightOf="@+id/textView56"
        app:layout_constraintLeft_toLeftOf="@+id/textView56" />

    <TextView
        android:id="@+id/tv_sat"
        android:layout_width="25dp"
        android:layout_height="25dp"
        android:layout_marginTop="8dp"
        android:gravity="center"
        android:text="22"
        android:textColor="@color/text_color"
        app:layout_constraintLeft_toLeftOf="@+id/textView21"
        app:layout_constraintRight_toRightOf="@+id/textView21"
        app:layout_constraintTop_toBottomOf="@+id/textView21" />

    <TextView
        android:id="@+id/tv_fri"
        android:layout_width="25dp"
        android:layout_height="25dp"
        android:layout_marginTop="8dp"
        android:text="21"
        android:gravity="center"
        android:textColor="@color/text_color"
        app:layout_constraintTop_toBottomOf="@+id/textView60"
        app:layout_constraintRight_toRightOf="@+id/textView60"
        app:layout_constraintLeft_toLeftOf="@+id/textView60" />

    <TextView
        android:id="@+id/tv_thu"
        android:layout_width="25dp"
        android:layout_height="25dp"
        android:layout_marginTop="8dp"
        android:gravity="center"
        android:text="20"
        android:textColor="@color/text_color"
        app:layout_constraintLeft_toRightOf="@+id/tv_wed"
        app:layout_constraintRight_toLeftOf="@+id/tv_fri"
        app:layout_constraintTop_toBottomOf="@+id/textView59" />

    <TextView
        android:id="@+id/tv_wed"
        android:layout_width="25dp"
        android:layout_height="25dp"
        android:layout_marginTop="8dp"
        android:text="今"
        android:background="@drawable/food_week_oval_shape"
        android:gravity="center"
        android:textColor="@color/text_color"
        app:layout_constraintTop_toBottomOf="@+id/textView58"
        app:layout_constraintRight_toRightOf="@+id/textView58"
        app:layout_constraintLeft_toLeftOf="@+id/textView58" />

    <TextView
        android:id="@+id/tv_mon"
        android:layout_width="25dp"
        android:layout_height="25dp"
        android:layout_marginTop="8dp"
        android:gravity="center"
        android:text="17"
        android:textColor="@color/text_color"
        app:layout_constraintLeft_toRightOf="@+id/tv_sun"
        app:layout_constraintRight_toLeftOf="@+id/tv_tue"
        app:layout_constraintTop_toBottomOf="@+id/textView55" />

    <TextView
        android:id="@+id/tv_sun"
        android:layout_width="25dp"
        android:layout_height="25dp"
        android:background="@color/bg_white"
        android:gravity="center"
        android:text="16"
        android:textColor="@color/text_color"
        tools:layout_editor_absoluteY="33dp"
        app:layout_constraintRight_toRightOf="@+id/textView22"
        app:layout_constraintLeft_toLeftOf="@+id/textView22" />

    <View
        android:layout_width="0dp"
        android:layout_height="10dp"
        android:background="@color/bg_grey"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/tv_sun"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent" />

</android.support.constraint.ConstraintLayout>